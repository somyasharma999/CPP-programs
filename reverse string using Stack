#include<iostream>
using namespace std;
int main(){
    string s;
    cout<<"Enter a string: ";
    cin>>s;
    int n=s.length();
    char stack[n];
    for(int i=0;i<n;i++){
        stack[i]=s[i];
    }
    cout<<"Reversed string: ";
    for(int i=n-1;i>=0;i--){
        cout<<stack[i];
    }
    return 0;
}
